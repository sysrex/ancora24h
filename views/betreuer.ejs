                                <div class="row clearfix">
                                    <div class="col-md-4 col-sm-4 col-xs-12">
                                        <div class="form-group">
                                            <input type="text" name="form_name" class="form-control" value="" placeholder="Name" required="">
                                        </div>
                                    </div>
                                    <div class="col-md-4 col-sm-4 col-xs-12">
                                        <div class="form-group">
                                            <input type="email" name="form_email" class="form-control required email" value="" placeholder="Email" required="">
                                        </div>
                                    </div>
                                    <div class="col-md-4 col-sm-4 col-xs-12">
                                            <div class="form-group">
                                                <input type="text" name="form_phone" class="form-control required phone" value="" placeholder="Phone" required="">
                                            </div>
                                    </div>
                                    <div class="col-md-12 col-sm-12 col-xs-12">
                                        <div class="form-group">
                                            <textarea name="form_message" class="form-control textarea required" placeholder="message"></textarea>
                                        </div>
                                    </div>   
                                    <div class="col-md-12 col-sm-12 col-xs-12">
                                        <div class="form-group form-bottom">
                                            <button class="btn-style-two" type="submit" data-loading-text="Please wait...">SEND MESSAGE</button>
                                        </div>
                                    </div>
                                </div>
